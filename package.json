  "scripts": {
    "start": "craco start",
    "serve": "serve -s build",
    "build": "craco build",
    "snap": "react-snap",
    "remove-extra": "node ./scripts/remove-build-data.js",
    "build:snap": "npm run build && npm run snap && npm run remove-extra && rimraf ./build/200.html",
    "i18n:extract": "i18next --config ./src/lang/i18next-parser.config.js 'src/**/*.{ts,tsx}' && prettier --write ./src/lang/translation/**",
    "i18n:new-language": "node ./scripts/inject_lang/index.js",
    "eslint": "eslint --ext js,ts,tsx",
    "stylelint": "stylelint \"src/styles/**/*.{css,scss,sass}\"",
    "checkTs": "tsc --noEmit",
    "lint": "npm run eslint src && npm run stylelint",
    "lint:fix": "npm run eslint -- --fix src npm && run stylelint -- --fix",
    "pretty:check": "prettier --check .",
    "pretty:fix": "prettier --write .",
    "predeploy": "npm run build:snap",
    "deploy": "gh-pages -d build",
    "test": "craco test",
    "eject": "react-scripts eject"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@tailwindcss/postcss7-compat": "2.2.17",
