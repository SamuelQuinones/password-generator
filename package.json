    "url": "https://github.com/SamuelQuinones/password-generator"
  },
  "bugs": {
    "url": "https://github.com/SamuelQuinones/password-generator/issues"
  },
  "dependencies": {
    "@reduxjs/toolkit": "1.7.1",
    "@testing-library/jest-dom": "5.16.1",
    "@testing-library/react": "12.1.2",
    "@testing-library/user-event": "13.5.0",
    "classnames": "2.3.1",
    "framer-motion": "5.5.5",
    "i18next": "21.6.5",
    "i18next-browser-languagedetector": "6.1.2",
    "react": "17.0.2",
    "react-accessible-dropdown-menu-hook": "3.1.0",
    "react-collapse": "5.1.1",
    "react-copy-to-clipboard": "5.0.4",
    "react-dom": "17.0.2",
    "react-hook-form": "7.22.5",
    "react-i18next": "11.15.3",
    "react-modal": "3.14.4",
    "react-redux": "7.2.6",
    "react-scripts": "5.0.0",
    "sass": "1.46.0",
    "web-vitals": "2.1.2",
    "workbox-background-sync": "6.4.2",
    "workbox-broadcast-update": "6.4.2",
    "workbox-cacheable-response": "6.4.2",
    "workbox-core": "6.4.2",
    "workbox-expiration": "6.4.2",
    "workbox-google-analytics": "6.4.2",
    "workbox-navigation-preload": "6.4.2",
    "workbox-precaching": "6.4.2",
    "workbox-range-requests": "6.4.2",
    "workbox-routing": "6.4.2",
    "workbox-strategies": "6.4.2",
    "workbox-streams": "6.4.2"
  },
  "scripts": {
    "start": "react-scripts start",
    "serve": "serve -s build",
    "build": "react-scripts build",
    "snap": "react-snap",
    "remove-extra": "node ./scripts/remove-build-data.js",
    "build:snap": "npm run build && npm run snap && npm run remove-extra && rimraf ./build/200.html",
    "i18n:extract": "i18next --config ./src/lang/i18next-parser.config.js 'src/**/*.{ts,tsx}' && prettier --write ./src/lang/translation/**",
    "i18n:new-language": "node ./scripts/inject_lang/index.js",
    "eslint": "eslint --ext js,ts,tsx",
    "stylelint": "stylelint \"src/styles/**/*.{css,scss,sass}\"",
    "checkTs": "tsc --noEmit",
    "lint": "npm run eslint src && npm run stylelint",
    "lint:fix": "npm run eslint -- --fix src npm && run stylelint -- --fix",
    "pretty:check": "prettier --check .",
    "pretty:fix": "prettier --write .",
    "pretty:theme": "prettier --write \"(src/(styles/**/*.{css,scss}|components/ThemeSwitcher/**/*.{ts,tsx})|scripts/generators/**/*.{ts,js})\"",
    "predeploy": "npm run build:snap",
    "deploy": "gh-pages -d build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "generate:theme": "plop --plopfile scripts/generators/plopfile.ts theme -- --overrideExisting N && npm run pretty:theme",
    "regenerate:theme": "rimraf src/styles/themes/* && ts-node scripts/generators/theme/regenerate.ts && npm run pretty:theme"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@types/jest": "27.4.0",
    "@types/node": "16.11.10",
    "@types/react": "17.0.38",
    "@types/react-collapse": "5.0.1",
    "@types/react-copy-to-clipboard": "5.0.2",
    "@types/react-dom": "17.0.11",
    "@types/react-modal": "3.13.1",
    "@typescript-eslint/eslint-plugin": "5.9.0",
    "@typescript-eslint/parser": "5.9.0",
    "autoprefixer": "10.4.1",
    "eslint": "8.6.0",
    "eslint-config-prettier": "8.3.0",
    "eslint-plugin-prettier": "4.0.0",
    "gh-pages": "3.2.3",
    "i18next-parser": "5.3.0",
    "node-plop": "0.26.2",
    "plop": "2.7.6",
    "postcss": "8.4.5",
    "prettier": "2.5.1",
    "react-snap": "1.23.0",
    "rimraf": "3.0.2",
    "serve": "13.0.2",
    "stylelint": "14.2.0",
    "stylelint-config-recommended": "6.0.0",
    "stylelint-config-standard-scss": "3.0.0",
    "tailwindcss": "2.2.19",
    "ts-node": "10.4.0",
    "typescript": "4.4.4"
  }
}
