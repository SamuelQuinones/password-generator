    "autoprefixer": "9.8.6",
    "eslint": "7.32.0",
    "eslint-config-prettier": "8.3.0",
    "eslint-plugin-prettier": "4.0.0",
    "gh-pages": "3.2.3",
    "i18next-parser": "5.0.0",
    "postcss": "7.0.36",
    "prettier": "2.4.1",
    "react-snap": "1.23.0",
    "rimraf": "3.0.2",
    "serve": "12.0.1",
    "stylelint": "13.13.1",
    "stylelint-config-recommended": "5.0.0",
