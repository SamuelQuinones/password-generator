    "react-accessible-dropdown-menu-hook": "3.2.0",
    "react-copy-to-clipboard": "5.1.0",
    "react-dom": "18.2.0",
    "react-hook-form": "7.33.0",
    "react-i18next": "11.17.4",
    "react-modal": "3.15.1",
    "react-redux": "8.0.2",
    "react-scripts": "5.0.1",
    "sass": "1.53.0",
    "web-vitals": "2.1.4",
    "workbox-background-sync": "6.5.3",
    "workbox-broadcast-update": "6.5.3",
    "workbox-cacheable-response": "6.5.3",
    "workbox-core": "6.5.3",
    "workbox-expiration": "6.5.3",
    "workbox-google-analytics": "6.5.3",
    "workbox-navigation-preload": "6.5.3",
    "workbox-precaching": "6.5.3",
    "workbox-range-requests": "6.5.3",
    "workbox-routing": "6.5.3",
    "workbox-strategies": "6.5.3",
    "workbox-streams": "6.5.3"
  },
  "scripts": {
    "start": "react-scripts start",
    "serve": "serve -s build",
    "build": "react-scripts build",
    "i18n:extract": "i18next --config ./scripts/i18next-parser.config.js 'src/**/*.{ts,tsx}' && prettier --write ./src/lang/translation/**",
    "i18n:new-language": "node ./scripts/inject_lang/index.js",
    "eslint": "eslint --ext js,ts,tsx",
    "checkTs": "tsc --noEmit",
    "lint": "npm run eslint src && prettier --check \"src/styles/**/*.{css,scss}\"",
    "lint:fix": "npm run eslint -- --fix src && prettier --write \"src/styles/**/*.{css,scss}\"",
    "pretty:check": "prettier --check .",
    "pretty:fix": "prettier --write .",
    "deploy": "gh-pages -d build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "generate": "plop --plopfile scripts/generators/plopfile.ts",
    "regenerate": "ts-node scripts/generators/regenfile.ts",
    "generate:theme": "npm run generate -- theme -- --overrideExisting N",
    "regenerate:theme": "rimraf src/styles/themes/* && npm run regenerate -- theme"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@types/jest": "28.1.3",
    "@types/node": "16.11.42",
    "@types/react": "18.0.14",
    "@types/react-copy-to-clipboard": "5.0.2",
    "@types/react-dom": "18.0.5",
    "@types/react-modal": "3.13.1",
    "@typescript-eslint/eslint-plugin": "5.30.0",
    "@typescript-eslint/parser": "5.30.0",
    "autoprefixer": "10.4.7",
    "cross-env": "7.0.3",
    "eslint": "8.18.0",
    "eslint-config-prettier": "8.5.0",
    "eslint-plugin-prettier": "4.2.1",
    "gh-pages": "4.0.0",
    "i18next-parser": "6.5.0",
    "node-plop": "0.26.2",
    "plop": "2.7.6",
    "postcss": "8.4.14",
    "prettier": "2.7.1",
    "prettier-plugin-tailwindcss": "0.1.11",
    "rimraf": "3.0.2",
    "serve": "13.0.2",
    "tailwindcss": "3.1.4",
    "ts-node": "10.8.1",
    "typescript": "4.7.4"
  }
}
